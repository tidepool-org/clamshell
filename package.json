{
  "name": "clamshell",
  "version": "0.4.2",
  "description": "Blip's mobile companion app for messaging",
  "private": true,
  "repository": "https://github.com/tidepool-org/clamshell.git",
  "author": "Tidepool",
  "scripts": {
    "test": "webpack-dev-server 'mocha!./test/entry.js' --output-file test.js --devtool eval-source-map --cache --colors --progress",
    "start": "webpack-dev-server --devtool eval-source-map --cache --colors --progress --port 3001",
    "build": "npm run build-app && npm run build-config",
    "build-app": "node buildapp",
    "build-config": "node buildconfig",
    "server": "node server",
    "jshint": "gulp jshint",
    "jshint-watch": "gulp jshint-watch"
  },
  "dependencies": {
    "async": "0.9.0",
    "autoprefixer-loader": "1.2.0",
    "bows": "1.4.2",
    "connect": "3.3.5",
    "css-loader": "0.9.1",
    "director": "1.2.8",
    "envify": "3.4.0",
    "fastclick": "1.0.6",
    "file-loader": "0.8.1",
    "hakken": "0.1.7",
    "imports-loader": "0.6.3",
    "json-loader": "0.5.1",
    "jsx-loader": "0.12.2",
    "less": "2.4.0",
    "less-loader": "2.1.0",
    "lodash": "3.5.0",
    "ms": "0.7.0",
    "react": "0.12.2",
    "serve-static": "1.9.2",
    "shelljs": "0.4.0",
    "style-loader": "0.8.3",
    "sundial": "1.1.7",
    "tidepool-platform-client": "0.16.5",
    "url-loader": "0.5.5",
    "webpack": "1.7.3"
  },
  "devDependencies": {
    "chai": "2.1.2",
    "gulp": "3.8.11",
    "gulp-jshint": "1.9.4",
    "gulp-react": "3.0.1",
    "jshint-stylish": "1.0.1",
    "mocha": "2.2.1",
    "mocha-loader": "0.7.1",
    "script-loader": "0.6.1",
    "sinon-chai": "2.7.0",
    "webpack-dev-server": "1.7.0"
  }
}
